//Arduino standard runs once
void setup() {
  Serial.begin(9600);
  randomSeed(analogRead(0));//Only needed for random screensaver (not curently used)

  //start shields
  AFMS1.begin();
  AFMS2.begin();
  AFMS3.begin(); 

  delay(500); // Give it time just in case...

  //populate Bubbler array - Yes, I wired it up mirror. No, I wasn't about to rewire it!
  Bubbler[0] = Motor11;
  Bubbler[1] = Motor10;
  Bubbler[2] = Motor9;
  Bubbler[3] = Motor8;
  Bubbler[4] = Motor7;
  Bubbler[5] = Motor6;
  Bubbler[6] = Motor5;
  Bubbler[7] = Motor4;
  Bubbler[8] = Motor3;
  Bubbler[9] = Motor2;
  Bubbler[10] = Motor1;
  Bubbler[11] = Motor0;

  // initialise channels
  for (int i = 0; i < NOOFCHANNELS; i++)
  {
    Bubbler[i]->setSpeed(DUTY); // Limits drive voltage for sub 5V valves
    Bubbler[i]->run(RELEASE); //This means off/valve closed
  }
  writestring("Hi!"); //"Hello World!" takes too long in repetitive testing

}

//Arduino standard runs repeatedly
void loop()
{
  if (Serial.available() > 0) //Look for new string
  {
    writestring(Serial.readStringUntil('\n')); // Write string to bubbles
    timelastaction = millis(); //reset screensaver timer
  }
  else if (hasdurationpassed(timelastaction, BOREDOMDELAY)) //Is it time for a screensaver?
  {
    bored(); // do something to alleviate boredom
  }
}

// Timing that is robust to millis rollover (if you leave it plugged in for 52 days!)
boolean hasdurationpassed(unsigned long timethen, unsigned long duration)
{
  return (boolean)((unsigned long)(millis() - timethen) > duration);
}

//Turn off all channels
void alloff()
{
  for (int i = 0; i < NOOFCHANNELS; i++)
  {
    Bubbler[i]->run(RELEASE); // off in Adafruit MotorShield library language
  }
}

//Take string and write characters to bubbles
void writestring(String s1)
{
  for (int i = 0; i < s1.length(); i++)
  {
    if (s1[i] < 128 && s1[i] > 31) // if it is in the ASCII 32-127 represented in the fonts
    {
      writechar(s1[i]);
      delay(LETTERPAUSE);
    }
    else // ` etc are not in the ASCII 32-127 represented in the fonts. Anything out of range is replaced with '?'
    {
      writechar('?');
      delay(LETTERPAUSE);
    }
  }
}

//Take each character and write two bytes fr each column
void writechar(char c1)
{
  for (int i = 0; i < FONTWIDTH; i++)
  {
    if ((pgm_read_byte(&(FONT[((2 * FONTWIDTH) + 1) * (c1 - 32) + (2 * i) + 2]))) || (pgm_read_byte(&(FONT[((2 * FONTWIDTH) + 1) * (c1 - 32) + (2 * i) + 1]))) || i < 2)// ensure it is not the end of the character
    {
      writebytes((pgm_read_byte(&(FONT[((2 * FONTWIDTH) + 1) * (c1 - 32) + (2 * i) + 2]))), (pgm_read_byte(&(FONT[((2 * FONTWIDTH) + 1) * (c1 - 32) + (2 * i) + 1])))); //finds and writes the relevant bytes from Font in progmem (starts at Ascii32) (Also ignores spare byte from GLCD fonts)
      delay(TIMEON);
      alloff();
      delay(TIMEOFF);
    }
    //else break; // removes space after letters - this was commented out because gaps were introduced to compensate for large horizontals moving to fast, but I'm not sure why the if statement still being there didn't cause issues... I guess it just reduces mid character pauses
  }
  if (VERBOSE) {
    Serial.println(F(" "));//space between letters in serial simulation
  }
}

// Takes the 12 least significant bits and injects bubbles on the relevant channel if a 1 is present
void writebytes(byte b1, byte b2)
{
  for (int i = NOOFCHANNELS - 9; i >= 0; i--) //go through each channel channel end of byte b1 msb first
  {
    if (b1 & (1 << i)) //if relevant bit is 1
    {
      //turn on stream i
      Bubbler[i + 8]->run(FORWARD);
      if (VERBOSE) {//Visualises bubble streams to the serial output
        Serial.print(F("#"));
      }
    }
    else // bit is zero
    {
      //turn off stream i
      Bubbler[i + 8]->run(RELEASE);
      if (VERBOSE) {//Visualises bubble streams to the serial output
        Serial.print(F(" "));
      }
    }
  }
  for (int i = 7; i >= 0; i--) //go through byte b2 msb first
  {
    if (b2 & (1 << i)) //if relevant bit is 1
    {
      //turn on stream i
      Bubbler[i]->run(FORWARD);
      if (VERBOSE) { //Visualises bubble streams to the serial output
        Serial.print(F("#"));
      }
    }
    else // bit is zero
    {
      //turn off stream i
      Bubbler[i]->run(RELEASE);
      if (VERBOSE) {//Visualises bubble streams to the serial output
        Serial.print(F(" "));
      }
    }
  }
  if (VERBOSE) {
    Serial.println(F(" "));
  }
}

