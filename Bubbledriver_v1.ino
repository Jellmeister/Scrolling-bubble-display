#include <Wire.h> //I2C library
#include <Adafruit_MotorShield.h> //Motorshield library
#define TIMEON 20UL //milliseconds valve is open to inject bubbles
#define TIMEOFF 200UL //milliseconds valve is closed between bubble injections
#define LETTERPAUSE 500UL // milliseconds pause between letters
#define DUTY 150 //out of 255, approximates the 3v the valves are designed for rather than the 5v minimum from the motor shields
#define BOREDOMDELAY 5000UL // milliseconds to time out to start writing stuff as a screensaver
#define REPEATBOREDOMOFFSET 0UL// millisecond reduction in boredom delay for repeat offenders (i.e. delay can be reduced one screeen saver has started
#define NOOFCHANNELS 12 //max 12 unless more shields are declared, never really used this with fewereven so beware of changing!
#define VERBOSE false // true streams Ascii art simulation of bubbler to serial. Compensation for bubble dynamics makes this look a bit odd
#define FONTWIDTH 11// tells the code how many columns a font has. Get this wrong and it chops the font bytes up incorrectly.
#define FONT Caps11 //AdjC11,CentGoth11,Tahoma10, Tahoma11B, TahomaAdj11 - which font set to use. Numbers in the font names are the appropriate font width to use

Adafruit_DCMotor* Bubbler[NOOFCHANNELS]; // I'm rubbish with pointers but this seems to work... array for controlling different channels by number
Adafruit_MotorShield AFMS1(0x60); // Default address, no jumpers
Adafruit_MotorShield AFMS2(0x61); // Solder bridge jumper on LSB
Adafruit_MotorShield AFMS3(0x62); // Solder Bridge jumper on second LSB

unsigned long timelastaction = 0;// screensaver interaction time monitor


Adafruit_DCMotor *Motor0 = AFMS1.getMotor(1);
